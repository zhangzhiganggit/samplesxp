<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
	
	<!-- ************ Dependency ************ -->
    <!-- 1 dao-datasource.xml                 -->
    <!-- ************************************ -->
    
    <!-- UUID -->
    <bean id="uuidGenerator" class="com.suixingpay.common.core.generator.id.uuid.SimpleUuidGenerator"/>
    
	<bean id="abstractIdGenerator" abstract="true" class="com.suixingpay.sms.system.generator.DefaultIdGenerator">
		<property name="dataSource" ref="smsWriteDataSource" />
	</bean>
	
	<bean id="testIdGenerator" parent="abstractIdGenerator" >
	    <constructor-arg value="TEST_ID"/>
	</bean>
	
	
	<bean id="uapOrgNoGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="UAP_ORG_NO"/>
	</bean>
	
	<bean id="virtualAccountIdGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="VIRTUAL_ACCOUNT"/>
	</bean>
	<bean id="goodsTransferIdGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="GOODS_TRANSFER_ID"/>
	</bean>
	<bean id="goodsTransferDetailIdGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="GOODS_TRANSFER_DETAIL_ID"/>
	</bean>
	<bean id="goodsOrderIdGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="GOODS_ORDER_ID"/>
	</bean>
	<bean id="goodsOrderDetailIdGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="GOODS_ORDER_DETAIL_ID"/>
	</bean>
	<bean id="goodsPriceIdGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="GOODS_PRICE_ID"/>
	</bean>
	<bean id="branchProfitIdGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="BRANCH_PROFIT"/>
	</bean>
	
	<bean id="agreementNoGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="AGREEMENT_NO"/>
	</bean>
	<bean id="creditIdGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="CREDIT_ID"/>
	</bean>
	<bean id="factSnGenerator" parent="abstractIdGenerator">
	    <constructor-arg value="FACT_SN"/>
	</bean>
</beans>