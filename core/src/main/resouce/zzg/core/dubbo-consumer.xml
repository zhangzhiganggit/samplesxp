<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd 
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd
	">

	<util:properties id="conf" location="classpath:config/application.properties" />

	<dubbo:application name="sms-front-reference" />
	<dubbo:registry address="#{conf['dubbo.registry.address']}" />
	<dubbo:protocol name="#{conf['dubbo.provider.protocol']}"  port="#{conf['dubbo.provider.port']}" />
	<dubbo:annotation/>
	<dubbo:reference id="processInterfaceService" interface="org.gaea.eip.core.biz.oa.workflow.orm.service.ProcessInterfaceService"  retries="0" check="false"/>
	
</beans>
